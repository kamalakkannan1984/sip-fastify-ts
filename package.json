{
    "name": "fastify-rest-ts-boilerplate",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "dependencies": {
        "axios": "0.19.1",
        "bcryptjs": "2.4.3",
        "dotenv": "^6.1.0",
        "fastify": "2.11.0",
        "fastify-auth": "0.7.2",
        "fastify-cors": "3.0.0",
        "fastify-mongodb": "^2.0.0",
        "fastify-swagger": "2.5.0",
        "jsonwebtoken": "8.5.1",
        "mongodb": "^3.5.4",
        "mongoose": "^5.3.11",
        "nodemon": "^1.18.6",
        "ts-node": "^7.0.1",
        "typescript": "^3.1.6",
        "underscore": "^1.9.2",
        "uuid": "3.3.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.4",
        "@types/dotenv": "^6.1.0",
        "@types/jsonwebtoken": "^8.3.0",
        "@types/mongoose": "^5.3.1",
        "@types/mongodb": "^3.5.0",
        "@types/node": "^12.0.2",
        "pre-commit": "^1.2.2",
        "standard": "^14.0.2",
        "tap": "^12.6.5"
    },
    "scripts": {
        ".": "nodemon src/index.ts",
        "start": "ts-node src/index.ts",
        "start:prod": "node dist/index.js",
        "prod": "node index.js",
        "build": "tsc --build tsconfig.json",
        "docker:up": "docker-compose up --build",
        "watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts"
    }
}
